#!/bin/bash/bin/bash
ss=""
[[ -n $1 ]] && ss="-s $1"
for i in $(seq 1 100)
do 
adb $ss shell am start -n com.android.settings/.Settings
call adb shell input tap 641 181
ping localhost -n 2 > nul
call adb shell input tap 500 560
ping localhost -n 2 > nul
call adb shell input tap 641 181
ping localhost -n 2 > nul

